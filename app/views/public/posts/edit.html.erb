<h1>編集画面</h1>
<%= form_with model: @post,url: update_post_path(@post.id), method: :patch do |f| %>
  <h4>投稿画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>店舗名</h4>
  <%= f.text_field :name %>
  <h4>店舗URL</h4>
  <%= f.text_field :url %>
  <h4>コメント</h4>
  <%= f.text_area :introduction %>
  <h4>駐車場</h4>
  <%= f.text_field :parking %>
  <%= f.submit '保存' %>
<% end %>